            getchufanum: function (shouweishu, moweishu) {
                var _randobj_moweishu = _global_pagezxsobj.getrandnum(parseInt(moweishu, 10));
                //除数
                var _chushu = _global_pagezxsobj.getrandomnum(_randobj_moweishu.min, _randobj_moweishu.max);
                //商最大临界值
                var _bcsmax = Math.floor(Math.pow(10, shouweishu) / _chushu);
                //商最小临界值
                var _bcsmin = Math.ceil(Math.pow(10, (shouweishu - 1)) / _chushu);
                //商
                var _shang = _global_pagezxsobj.getrandomnum(_bcsmin, _bcsmax);
                //被除数
                var _bcs = _shang * _chushu;
                return { "bcs": _bcs, "cs": _chushu, "shang": _shang };
